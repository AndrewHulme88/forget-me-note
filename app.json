{
  "expo": {
    "name": "recollecto",
    "slug": "recollecto",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "plugins": ["expo-notifications"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gorlami.forgetmenote",
      "buildNumber": "17",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSUserNotificationUsageDescription": "Allow notifications so Recollecto can remind you about tasks."
      }
    },
    "android": {
      "package": "com.gorlami.forgetmenote",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": ["POST_NOTIFICATIONS"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f8318c11-7aa6-4afb-baf9-7e20b85d24b8"
      }
    }
  }
}
